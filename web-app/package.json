{
  "name": "nomnom-web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "15.5.7",
    "query-string": "4.3.2",
    "react": "15.5.4",
    "react-apollo": "1.1.2",
    "react-dom": "15.5.4",
    "react-google-login": "git://github.com/kombucha/react-google-login.git#update-to-latest-react",
    "react-icons": "2.2.3",
    "react-router-dom": "4.1.1",
    "styled-components": "2.0.0-17"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.4.0",
    "@kadira/storybook": "2.21.0",
    "react-scripts": "1.0.1",
    "source-map-explorer": "1.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage --collectCoverageFrom='[\"src/**/*.js\",\"!src/**/*.stories.js\"]' --coverageReporters='[\"json\", \"html\", \"text\"]'",
    "eject": "react-scripts eject",
    "analyse": "source-map-explorer build/static/js/main.*",
    "storybook": "start-storybook -p 9009 -s public --dont-track",
    "build-storybook": "build-storybook -s public"
  },
  "proxy": "http://localhost:4001"
}
